{
	"name": "BlueLL",
	"version": "2.2.0",
	"author": [
		"Antoine Lamielle"
	],
	"url": "https://github.com/lingua-libre/BlueLL",
	"descriptionmsg": "bluell-desc",
	"license-name": "BSD-2-Clause",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"ValidSkinNames": {
		"bluell": {
			"class": "SkinTemplate",
			"args": [
				{
					"name": "bluell",
					"responsive": true,
					"template": "BlueLLTemplate",
					"bodyOnly": true,
					"scripts": [
						"skins.bluell.js"
					],
					"styles": [
						"skins.bluell.styles"
					]
				}
			]
		}
	},
	"MessagesDirs": {
		"SkinBlueLL": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"SkinBlueLL": "BlueLL.skin.php",
		"BlueLLTemplate": "BlueLL.skin.php"
	},
	"ResourceModules": {
		"skins.bluell.styles": {
			"position": "top",
			"styles": [
				"assets/stylesheets/normalize.min.css",
				"assets/stylesheets/font-awesome.min.css",
				"assets/stylesheets/main.css",
				"assets/stylesheets/nav.css",
				"assets/stylesheets/content.css",
				"assets/stylesheets/footer.css",
				"assets/stylesheets/static.css",
				"assets/stylesheets/print.css",
				"assets/stylesheets/ext.echo.css"
			]
		},
		"skins.bluell.js": {
			"position": "bottom",
			"scripts": [
				"assets/scripts/bluell.js"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "BlueLL"
	},
	"config": {},
	"manifest_version": 1
}
